INICIO:
	la $s0, nota_dur
	la $s1, blablabla
	li $t0, 0
MUSICA:
	lw $a0, 0($s0)
	li $a1, 400 
	li $a2, 07
	li $a3, 127
	li $v0, 31
	syscall
	
	li $v0, 32
	lw $a0, 300
	syscall
	addi $s0, $s0, 8
	addi $s1, $s1, 4
	addi $t0, $t0, 1
	bne $t0, n, MUSICA
	j INICIO
fim:
	li $v0, 10
	syscall
